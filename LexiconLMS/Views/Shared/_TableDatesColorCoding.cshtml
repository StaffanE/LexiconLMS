<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Färgkodning datumintervall Start-Slut</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="@Url.Content("~/Content/Gridmvc.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/gridmvc.min.js")" type="text/javascript"> </script>
</head>
<body>
    <div class="container">
        <tr>
            <td bgcolor:"rgba(65,173,73,0.5)">
                Avslutad period
            </td>
            <td bgcolor:"rgba(247,247,247,0.5)">
                Innevarande period
            </td>
            <td bgcolor:"rgba(245,121,32,0.5)">
                Kommande period
            </td>
    </div>
    <div class="container">
        <table border="1">
            <tr style="background:rgba(247,247,247,0.5);">
                <td>Avslutad period           .</td>
            </tr>
        </table>
    </div>
    <div class="container">
        <table border="1">
            <tr style="background:rgba(65,173,73,0.5);">
                <td>Aktuell/innevarande period.</td>
            </tr>
        </table>
    </div>
    <div class="container">
        <table border="1">
            <tr style="background:rgba(245,121,32,0.5);">
                <td>Framtida/kommande period.</td>
            </tr>
        </table>
    </div>
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @*@RenderSection("scripts", required: false)*@
</body>
</html>
